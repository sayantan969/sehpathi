<script>
  document.addEventListener('DOMContentLoaded', function () {
    const headingText = "JOIN US TODAY AND GET ORGANISED "; 
    let index = 0;

    function type() {
        const heading = document.getElementById("h");

        if (heading && index < headingText.length) {
            heading.textContent += headingText[index];
            index++;
            setTimeout(type, 200); 
        }
    }

    
    type();
});

